{
    "name": "greeflas/yii2-i18n-module",
    "description": "Yii2 i18n (internalization) module makes the translation of your application so simple",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "i18n",
        "translation",
        "translations"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Zelenin",
            "email": "aleksandr@zelenin.me",
            "homepage": "http://zelenin.me",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/greeflas/yii2-i18n-module/issues",
        "source": "https://github.com/greeflas/yii2-i18n-module"
    },
    "require": {
        "yiisoft/yii2": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "greeflas\\yii\\modules\\I18n\\": ""
        }
    },
    "extra": {
        "bootstrap": "greeflas\\yii\\modules\\I18n\\Bootstrap"
    }
}
