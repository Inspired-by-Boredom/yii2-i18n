{
    "name": "greeflas/yii2-i18n",
    "description": "Yii2 i18n (internalization) module",
    "keywords": ["yii2", "i18n", "translation", "translations"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/greeflas/yii2-i18n-module/issues",
        "source": "https://github.com/greeflas/yii2-i18n-module"
    },
    "authors": [
        {
            "name": "Aleksandr Zelenin",
            "email": "aleksandr@zelenin.me",
            "homepage": "http://zelenin.me",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "greeflas\\i18n\\": "src"
        }
    },
    "extra": {
        "bootstrap": "greeflas\\i18n\\Bootstrap"
    }
}
